{
  "name": "rebox",
  "version": "0.1.0",
  "description": "A flexible box for React Native",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib --copy-files",
    "watch": "rm -rf lib && babel src --out-dir lib -w --copy-files",
    "lint": "eslint .",
    "test": "npm run lint",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madjam002/rebox.git"
  },
  "keywords": [
    "react",
    "flexible",
    "box",
    "native"
  ],
  "author": "Jamie Greeff <jamie@greeff.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/madjam002/rebox/issues"
  },
  "homepage": "https://github.com/madjam002/rebox#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "eslint": "^4.3.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  }
}
