{
  "name": "rebox",
  "version": "0.1.0",
  "description": "Flexible and easy layout for React and React Native",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir lib --copy-files",
    "watch": "rm -rf lib && babel src --out-dir lib -w --copy-files",
    "lint": "eslint .",
    "test": "npm run lint && npm run unit && npm run screenie-ci",
    "unit": "jest",
    "screenie": "screenie -r babel-register tests/test.js",
    "screenie-webpack": "webpack-dev-server --config tests/webpack.config.js",
    "screenie-ci": "selenium-standalone install && concurrently -k -r --success first \"npm run screenie\" \"bash ./resources/scripts/start-selenium.sh\" \"npm run screenie-webpack\"",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madjam002/rebox.git"
  },
  "keywords": [
    "react",
    "flexible",
    "box",
    "native"
  ],
  "author": "Jamie Greeff <jamie@greeff.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/madjam002/rebox/issues"
  },
  "homepage": "https://github.com/madjam002/rebox#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-native": "^3.0.1",
    "babel-preset-stage-1": "^6.24.1",
    "concurrently": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.55.0",
    "ip": "^1.1.5",
    "jest": "^20.0.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-native-web": "0.0.120",
    "react-router": "^3.0.5",
    "react-test-renderer": "^15.6.1",
    "screenie-cli": "^2.0.0",
    "screenie-core": "^2.0.0",
    "screenie-webdriver": "^2.0.0",
    "selenium-standalone": "^6.9.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "dependencies": {
    "styled-components": "^2.1.2"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/tests/"
    ]
  }
}
